{
	"compilerOptions": {
		"esModuleInterop": true,
		"lib": [
			"ESNext",
			"DOM"
		],
		"module": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "ESNext",
		"types": [
			"vite/client"
		],
		"useDefineForClassFields": true
	},
	"include": [
		"app"
	]
}